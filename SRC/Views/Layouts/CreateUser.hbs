<title>Start</title>

<body>
    <div class="container">
        <h1 class="Title">Create your user!</h1>
            <form id="userForm">
                <div class="create-user">
                    <div class="=inputButtons">
                        <input type="text" id="userInput" class="username" placeholder="digite um nome aqui" autocomplete="off">
                        <input type="text" id="roomInput" class="room" placeholder="digite a sala aqui">
                    </div>
                    <button type="submit" id="submitUser" class="submitButton">
                        <img src="/assets/icons/login.svg" alt="submit">
                    </button> 
                </div>
            </form>
    </div>
</body>

